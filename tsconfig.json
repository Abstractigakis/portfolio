{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@data/*": ["data/*"],
      "@lib/*": ["lib/*"],
      "@utils/*": ["utils/*"],
      "@components/*": ["components/*"],
      "@features/*": ["features/*"],
      "@middleware/*": ["lib/nextAuth/middleware/*"]
    },
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    "components/Box.jsx",
    "components/OrbitControls.jsx",
    "components/ErixThrees/Primitives/Floor.jsx",
    "components/ErixThrees/Primitives/Ball.jsx",
    "components/ErixThrees/ThreeContainer.jsx",
    "components/ErixThrees/Scenes/Minecraft/index.jsx",
    "pages/d.jsx",
    "components/ErixThrees/Minecraft/Prefabs/Player/Camera.jsx",
    "pages/index.jsx",
    "components/HomeAnimatedBG/index.jsx"
  ],
  "exclude": ["node_modules"]
}
